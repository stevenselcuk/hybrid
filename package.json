{
		"name": "hybrid",
		"version": "0.0.1",
		"description": "REST & Grapqhl (Apollo Server) Hybrid API Boilerplate",
		"main": "index.js",
		"repository": "https://github.com/stevenselcuk/hybrid.git",
		"author": "Steven J. Selcuk",
		"license": "MIT",
		"scripts": {
				"dev": "NODE_ENV=development PORT=3000 nodemon src/server.js --exec babel-node",
				"dev:webpack": "NODE_ENV=development PORT=3000 webpack --config ./webpack.config.dev.babel.js",
				"build:babel": "yarn clean && babel -d ./build ./src -s",
				"build": "NODE_ENV=production webpack --config ./webpack.config.production.babel.js",
				"start:babel": "NODE_ENV=production node build/server.js",
				"start": "NODE_ENV=production node index.js",
				"deploy": "yarn build:webpack && pm2 start pm2.config.js",
				"seed": "NODE_ENV=development babel-node tools/seeder/seeder.js",
				"test": "NODE_ENV=test nyc --reporter=html --reporter=text ./node_modules/.bin/mocha ./test/**/*.js --exit",
				"test:coverage": "NODE_ENV=test nyc --reporter=lcov ./node_modules/.bin/mocha ./test/**/*.js --exit",
				"doc": "./node_modules/.bin/jsdoc -c jsdoc.json",
				"swag": "swagger-jsdoc -d swaggerDef.js",
				"test:watch": "jest --watchAll",
				"mocha": "mocha ./test/**/*.js",
				"report-coverage": "NODE_ENV=test nyc ./node_modules/.bin/mocha ./test/**/*.js",
				"unit": "jest \"src/.*\\.js$\" --coverage --forceExit",
				"e2e": "cross-env JEST_ENV=e2e jest \"src/.*\\.js$\" --forceExit",
				"clean": "rm -rf build && mkdir build && pm2 kill && pm2 flush",
				"_codecov": "codecov",
				"lint": "eslint ./src/ --ignore-path .gitignore --ignore-pattern internals/scripts",
				"lint:fix": "yarn lint --fix",
				"lint:watch": "yarn lint --watch"
		},
