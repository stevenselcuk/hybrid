		"husky": {
				"hooks": {
						"pre-commit": "lint-staged",
						"pre-push": "yarn test"
				}
		},
		"lint-staged": {
				"*.{js}": [
						"eslint --fix ./src",
						"git add"
				]
		},
		"engines": {
				"node": "14.x",
				"npm": "^6.13.6",
				"yarn": "^1.15.2"
		},
		"nodemonConfig": {
				"watch": [
						"src"
				],
				"ext": "js",
				"ignore": [
						"*.test.js"
				],
				"delay": "3",
				"execMap": {
						"js": "babel-node"
				}
		}
